if #parentName["controlledChildren"] > 1 then
	for i = 2,#parentName["controlledChildren"] do
		if parentName["controlledChildren"][i] then
			WeakAuras.ActivateAuraEnvironment(parentName["controlledChildren"][i])
			WeakAuras.ActivateAuraEnvironment()
		end
	end
end

