-----------------------------------------------------------------------------------------------
--------- DON'T CHANGE THESE SETTINGS, THEY ARE REQUIRED FOR THE WEAKAURA TO FUNCTION ---------
-----------------------------------------------------------------------------------------------

-- converts RGB to hex
aura_env.RGBPercToHex = function(r, g, b)
	r = r <= 1 and r >= 0 and r or 0
	g = g <= 1 and g >= 0 and g or 0
	b = b <= 1 and b >= 0 and b or 0
	return string.format("%02x%02x%02x", r*255, g*255, b*255)
end

-- colors green, yellow, and scaling red -> yellow
aura_env.colorByTime = function(time)
	if (time <= 0) then
		return "|cFF00FF00";
	elseif (time <= 15) then
		return "|cFFFFFF00";
	elseif (time <= 90) then
		return "|cFF"..aura_env.RGBPercToHex(1, 1- ((time - 15)/75), 0);
	else
		return "|cFFFF0000";
	end  
end

-- sorts table
aura_env.sortedPairs = function(t) 
	if TehrsCDs["Show Settings"].SortByTimer then
		local keys = {}
		for k in pairs(t) do
			keys[#keys+1] = k
		end
		table.sort(keys, function(a,b)
			return t[b] > t[a]
		end)
		local i = 0
		return function()
			i = i + 1
			if keys[i] then
				return keys[i], t[keys[i]]
			end
		end
	else
		local keys = {}; 
		for k in pairs(t) do
			table.insert(keys, k)
		end 
		table.sort(keys, function(a,b) 
				if tonumber(a) and tonumber(b) then 
					return a > b; 
				else 
					return tostring(a)>tostring(b); 
				end 
		end) 
		local i = 0; 
		return function() 
			if i < #keys then 
				i = i + 1; 
				return keys[i], t[keys[i]]; 
			end 
		end 
	end
end

-- adds pet abilities to the display
aura_env.CheckPetAbilities = function(currentPlayer,currentPet,currentPlayerClass,activeSpec,isPlayer)
	if (TehrsCDs._interrupts_warlocks == nil) then TehrsCDs._interrupts_warlocks = {} end
	if (TehrsCDs._interrupts_warlocks[currentPlayer] == nil) then TehrsCDs._interrupts_warlocks[currentPlayer] = {} end         
	local _, _, _, sacSpecced = GetTalentInfo(6, 3, activeSpec, not isPlayer, playerName);  
	
	if (GetLocale() == "enUS") or (GetLocale() == "enGB") then
		if UnitExists(currentPet) then
			if TehrsCDs["Show Settings"].SpellLock then
				if UnitCreatureFamily(currentPet) == "Felhunter" then
					if (TehrsCDs._interrupts_warlocks[currentPlayer]["Spell Lock"] == nil) then
						TehrsCDs._interrupts_warlocks[currentPlayer]["Spell Lock"] = GetTime();
					end
					if TehrsCDs.DEBUG and TehrsCDs.DEBUG_GroupPoll then print("GroupPoll: added Felhunter Spell Lock CD") end
				else
					TehrsCDs._interrupts_warlocks[currentPlayer]["Spell Lock"] = nil;
				end
			else
				TehrsCDs._interrupts_warlocks[currentPlayer]["Spell Lock"] = nil;
			end
		elseif not sacSpecced then	
			TehrsCDs._interrupts_warlocks[currentPlayer]["Spell Lock"] = nil;
		end
	end
end

-- returns pet owner
aura_env.getPetOwner = function(petName, petGUID)
	local ownerName
	
	if UnitGUID("pet") == petGUID then
		ownerName = GetUnitName("player")
	elseif IsInRaid() then
		for i=1, GetNumGroupMembers() do
			if UnitGUID("raid"..i.."pet") == petGUID then
				ownerName = GetUnitName("raid"..i)
				break
			end
		end
	else
		for i=1, GetNumSubgroupMembers() do
			if UnitGUID("party"..i.."pet") == petGUID then
				ownerName = GetUnitName("party"..i)
				break
			end
		end
	end
	
	if ownerName then
		return ownerName
	else
		return petName
	end
end   

