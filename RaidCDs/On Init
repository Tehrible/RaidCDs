-- Buttons
aura_env.alphaSlider = aura_env.alphaSlider or CreateFrame("Slider","Background Alpha",region,"OptionsSliderTemplate") --frameType, frameName, frameParent, frameTemplate   
aura_env.alphaSlider:SetPoint("TOP",0,35)
aura_env.alphaSlider.textLow = _G[backgroundAlpha.."Low"]
aura_env.alphaSlider.textHigh = _G[backgroundAlpha.."High"]
aura_env.alphaSlider.text = _G[backgroundAlpha.."Text"]
aura_env.alphaSlider:SetMinMaxValues(0, 1)
aura_env.alphaSlider.minValue, aura_env.alphaSlider.maxValue = aura_env.alphaSlider:GetMinMaxValues() 
aura_env.alphaSlider.textLow:SetText(aura_env.alphaSlider.minValue)
aura_env.alphaSlider.textHigh:SetText(aura_env.alphaSlider.maxValue)
aura_env.alphaSlider.text:SetText(backgroundAlpha)
aura_env.alphaSlider:SetValue(.5)
aura_env.alphaSlider:SetValueStep(.1)
aura_env.alphaSlider:SetScript("OnValueChanged", function(self,event,arg1) bar.bg:SetVertexColor(0,0,0,event) end)
aura_env.alphaSlider:Hide()

aura_env.textSlider = aura_env.textSlider or CreateFrame("Slider","Text Size",aura_env.alphaSlider,"OptionsSliderTemplate") --frameType, frameName, frameParent, frameTemplate   
aura_env.textSlider:SetPoint("TOP",0,35)
aura_env.textSlider.textLow = _G[textSize.."Low"]
aura_env.textSlider.textHigh = _G[textSize.."High"]
aura_env.textSlider.text = _G[textSize.."Text"]
aura_env.textSlider:SetMinMaxValues(11, 24)
aura_env.textSlider.minValue, aura_env.textSlider.maxValue = aura_env.textSlider:GetMinMaxValues() 
aura_env.textSlider.textLow:SetText(aura_env.textSlider.minValue)
aura_env.textSlider.textHigh:SetText(aura_env.textSlider.maxValue)
aura_env.textSlider.text:SetText(textSize)
aura_env.textSlider:SetValue(fontHeight)
aura_env.textSlider:SetValueStep(1)
aura_env.textSlider:SetScript("OnValueChanged", function(self,event,arg1)
	region.text:SetFont(filename,event,flags) 
	TehrsCDs.fontSize = event
end)
aura_env.textSlider:SetScript("OnMouseUp", function(self,event,arg1)
	print("|cFF00A2E8Tehr's RaidCDs:|r Reload your UI to re-align!")
	WeakAurasSaved.displays[aura_env.id]["fontSize"] = TehrsCDs.fontSize
end)

aura_env.maximizeButton = aura_env.maximizeButton or CreateFrame("Button","mymaximizeButton",region,"UIPanelButtonTemplate")
aura_env.maximizeButton:SetPoint(point, region, point, xOffset, yOffset)
aura_env.maximizeButton:SetWidth(50)
aura_env.maximizeButton:SetHeight(20)
aura_env.maximizeButton:SetText("show")
aura_env.maximizeButton:SetNormalFontObject("GameFontNormalSmall")
aura_env.maximizeButton:SetDisabledFontObject(GameFontDisable)
aura_env.maximizeButton:SetHighlightFontObject(GameFontHighlight)
aura_env.maximizeButton:SetNormalFontObject(GameFontNormal)
aura_env.maximizeButton:RegisterForClicks("AnyDown")
aura_env.maximizeButton:SetScript("OnClick", function (self, button, down)
	TehrsCDs.minmaxDisplay = true
	TehrsCDs.BackgroundAlpha = TehrsCDs.BackgroundAlpha or 0
	bar.bg:SetVertexColor(0,0,0,TehrsCDs.BackgroundAlpha)
	print("|cFF00A2E8Tehr's RaidCDs:|r "..( TehrsCDs.minmaxDisplay and "|cFF00FF00"..buttonText["printEnabled"].."|r" or "|cFFFF0000"..buttonText["printDisabled"].."|r" ));
	if aura_env.parentButton then aura_env.parentButton:Show() end
	aura_env.maximizeButton:Hide()
end)

if showParentButton then
	aura_env.parentButton = aura_env.parentButton or CreateFrame("Button","myparentButton",region,"UIPanelButtonTemplate")
	aura_env.parentButton:SetPoint("TOPRIGHT",0,0)
	aura_env.parentButton:SetWidth(20)
	aura_env.parentButton:SetHeight(20)
	aura_env.parentButton:SetText("S")
	aura_env.parentButton:EnableMouse(true)
	aura_env.parentButton:SetFrameLevel(backgroundFrameLevel + 1)
	aura_env.parentButton:SetNormalFontObject("GameFontNormalSmall")
	aura_env.parentButton:SetDisabledFontObject(GameFontDisable)
	aura_env.parentButton:SetHighlightFontObject(GameFontHighlight)
	aura_env.parentButton:SetNormalFontObject(GameFontNormal)
	aura_env.parentButton:RegisterForClicks("AnyDown")
	aura_env.parentButton:SetScript("OnClick", function (self, button, down)
		if button == "LeftButton" then
			local menu = {    
				{ text = buttonText["L1N1"], notCheckable = 1, isTitle = true}, 
				
				--Toggle Display
				{ text = buttonText["L1N2"], notCheckable = 1,
					func = function()
						TehrsCDs.minmaxDisplay = false
						_,_,_,TehrsCDs.BackgroundAlpha = bar.bg:GetVertexColor()
						bar.bg:SetVertexColor(0,0,0,0)
						print("|cFF00A2E8Tehr's RaidCDs:|r "..( TehrsCDs.minmaxDisplay and "|cFF00FF00"..buttonText["printEnabled"].."|r" or "|cFFFF0000"..buttonText["printDisabled"].."|r" ));
						aura_env.maximizeButton:Show()
						aura_env.parentButton:Hide()
					end },   				
				
				-- Toggle Sliders
				{ text = buttonText["L1N3"], notCheckable = 1,
					func = function()
						if aura_env.alphaSlider:IsShown() then
							aura_env.alphaSlider:Hide()
						else
							aura_env.alphaSlider:Show()
						end
					end },	

				{ text = " ", notCheckable = 1, disabled = true},
					
				--Toggle Polling
				{ text = buttonText["L1N8"], isNotRadio = true, keepShownOnClick = true, checked = not (TehrsCDs._raidCDs_groupPoll_state == "pause"),
					func = function()
						if TehrsCDs._raidCDs_groupPoll_state == "pause" then
							TehrsCDs._raidCDs_groupPoll_state = "nextPlayer"
						else
							TehrsCDs._raidCDs_groupPoll_state = "pause"
						end
						print("|cFF00A2E8Tehr's RaidCDs:|r "..buttonText["L1N8"].." "..(TehrsCDs._raidCDs_groupPoll_state == "pause" and "|cFFFF0000"..buttonText["printDisabled"].."|r" or "|cFF00FF00"..buttonText["printEnabled"].."|r" ))
					end },						
						--[[
				--Toggle Sorting by Timer
				{ text = buttonText["L1N9"], isNotRadio = true, keepShownOnClick = true, checked = TehrsCDs["Show Settings"].SortByTimer,
					func = function()
						TehrsCDs["Show Settings"].SortByTimer = not TehrsCDs["Show Settings"].SortByTimer
						print("|cFF00A2E8Tehr's RaidCDs:|r "..buttonText["L1N9"].." "..(not TehrsCDs["Show Settings"].SortByTimer and "|cFFFF0000"..buttonText["printDisabled"].."|r" or "|cFF00FF00"..buttonText["printEnabled"].."|r" ))
					end },					
						]]
					
				--Toggle Empty Sections
				{ text = buttonText["L1N10"], isNotRadio = true, keepShownOnClick = true, checked = TehrsCDs["Show Settings"].ShowEmptySections,
					func = function()
						TehrsCDs["Show Settings"].ShowEmptySections = not TehrsCDs["Show Settings"].ShowEmptySections
						print("|cFF00A2E8Tehr's RaidCDs:|r "..buttonText["L1N10"].." "..( TehrsCDs["Show Settings"].ShowEmptySections and "|cFF00FF00"..buttonText["printEnabled"].."|r" or "|cFFFF0000"..buttonText["printDisabled"].."|r" ))
				end },          
			   
				--Toggle On Cool
				{ text = buttonText["L1N11"], isNotRadio = true, keepShownOnClick = true, checked = TehrsCDs["Show Settings"].ShowOnCooldownOnly,
					func = function()
						TehrsCDs["Show Settings"].ShowOnCooldownOnly = not TehrsCDs["Show Settings"].ShowOnCooldownOnly
						print("|cFF00A2E8Tehr's RaidCDs:|r "..buttonText["L1N11"].." "..( TehrsCDs["Show Settings"].ShowOnCooldownOnly and "|cFF00FF00"..buttonText["printEnabled"].."|r" or "|cFFFF0000"..buttonText["printDisabled"].."|r" ))
				end },					
					
