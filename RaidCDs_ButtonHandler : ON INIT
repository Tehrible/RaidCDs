                                    print("|cFF00A2E8Tehr's RaidCDs:|r "..buttonText["L2N5.4"].." "..( TehrsCDs["Show Settings"].allUtility and "|cFF00FF00"..buttonText["printEnabled"].."|r" or "|cFFFF0000"..buttonText["printDisabled"].."|r" ))
