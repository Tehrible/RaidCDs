-- Buttons

local alphaSlider = CreateFrame("Slider","Background Alpha",WeakAuras.regions[backgroundName].region,"OptionsSliderTemplate") --frameType, frameName, frameParent, frameTemplate   
alphaSlider:SetPoint("TOP",0,35)
alphaSlider.textLow = _G[backgroundAlpha.."Low"]
alphaSlider.textHigh = _G[backgroundAlpha.."High"]
alphaSlider.text = _G[backgroundAlpha.."Text"]
alphaSlider:SetMinMaxValues(0, 1)
alphaSlider.minValue, alphaSlider.maxValue = alphaSlider:GetMinMaxValues() 
alphaSlider.textLow:SetText(alphaSlider.minValue)
alphaSlider.textHigh:SetText(alphaSlider.maxValue)
alphaSlider.text:SetText(backgroundAlpha)
alphaSlider:SetValue(.5)
alphaSlider:SetValueStep(.1)
alphaSlider:SetScript("OnValueChanged", function(self,event,arg1) WeakAuras.regions[backgroundName].region.texture:SetVertexColor(0,0,0,event) end)
alphaSlider:Hide()

local textSlider = CreateFrame("Slider","Text Size",alphaSlider,"OptionsSliderTemplate") --frameType, frameName, frameParent, frameTemplate   
textSlider:SetPoint("TOP",0,35)
textSlider.textLow = _G[textSize.."Low"]
textSlider.textHigh = _G[textSize.."High"]
textSlider.text = _G[textSize.."Text"]
textSlider:SetMinMaxValues(11, 24)
textSlider.minValue, textSlider.maxValue = textSlider:GetMinMaxValues() 
textSlider.textLow:SetText(textSlider.minValue)
textSlider.textHigh:SetText(textSlider.maxValue)
textSlider.text:SetText(textSize)
textSlider:SetValue(fontHeight)
textSlider:SetValueStep(1)
textSlider:SetScript("OnValueChanged", function(self,event,arg1)
        WeakAuras.regions[namesTextName].region.text:SetFont(filename,event,flags) 
        TehrsCDs.fontSize = event
end)
textSlider:SetScript("OnMouseUp", function(self,event,arg1)
        print("|cFF00A2E8Tehr's RaidCDs:|r Reload your UI to re-align!")
        WeakAurasSaved.displays[namesTextName]["fontSize"] = TehrsCDs.fontSize
end)

aura_env.maximizeButton = aura_env.maximizeButton or CreateFrame("Button","mymaximizeButton",WeakAuras.regions[aura_env.id].region,"UIPanelButtonTemplate")
aura_env.maximizeButton:SetPoint("CENTER",0,0)
aura_env.maximizeButton:SetWidth(50)
aura_env.maximizeButton:SetHeight(20)
aura_env.maximizeButton:SetText("show")
aura_env.maximizeButton:SetNormalFontObject("GameFontNormalSmall")
aura_env.maximizeButton:SetDisabledFontObject(GameFontDisable)
aura_env.maximizeButton:SetHighlightFontObject(GameFontHighlight)
aura_env.maximizeButton:SetNormalFontObject(GameFontNormal)
aura_env.maximizeButton:RegisterForClicks("AnyDown")
aura_env.maximizeButton:SetScript("OnClick", function (self, button, down)
        TehrsCDs.minmaxDisplay = not TehrsCDs.minmaxDisplay
        print("|cFF00A2E8Tehr's RaidCDs:|r "..( TehrsCDs.minmaxDisplay and "|cFF00FF00"..buttonText["printEnabled"].."|r" or "|cFFFF0000"..buttonText["printDisabled"].."|r" ));
end)

if TehrsCDs_show_ParentButton then
    aura_env.parentButton = aura_env.parentButton or CreateFrame("Button","myparentButton",WeakAuras.regions[backgroundName].region,"UIPanelButtonTemplate")
    aura_env.parentButton:SetPoint("TOPRIGHT",0,0)
    aura_env.parentButton:SetWidth(20)
    aura_env.parentButton:SetHeight(20)
    aura_env.parentButton:SetText("S")
    aura_env.parentButton:EnableMouse(true)
    aura_env.parentButton:SetNormalFontObject("GameFontNormalSmall")
    aura_env.parentButton:SetDisabledFontObject(GameFontDisable)
    aura_env.parentButton:SetHighlightFontObject(GameFontHighlight)
    aura_env.parentButton:SetNormalFontObject(GameFontNormal)
    aura_env.parentButton:RegisterForClicks("AnyDown")
    aura_env.parentButton:SetScript("OnClick", function (self, button, down)
            if button == "LeftButton" then
                local menu = {    
                    { text = buttonText["L1N1"], notCheckable = 1, isTitle = true}, 
                    
                    --Toggle Display
                    { text = buttonText["L1N2"], notCheckable = 1,
                        func = function()
                            TehrsCDs.minmaxDisplay = not TehrsCDs.minmaxDisplay
                            print("|cFF00A2E8Tehr's RaidCDs:|r "..( TehrsCDs.minmaxDisplay and "|cFF00FF00"..buttonText["printEnabled"].."|r" or "|cFFFF0000"..buttonText["printDisabled"].."|r" ))
                    end },                   
                    
                    -- Toggle Sliders
                    { text = buttonText["L1N3"], notCheckable = 1,
                        func = function()
                            if alphaSlider:IsShown() then
                                alphaSlider:Hide()
                            else
                                alphaSlider:Show()
                            end
                    end },    
                    
                    { text = " ", notCheckable = 1, disabled = true},
                    
                    --Toggle Polling
                    { text = buttonText["L1N8"], isNotRadio = true, keepShownOnClick = true, checked = not (TehrsCDs._raidCDs_groupPoll_state == "pause"),
                        func = function()
                            if TehrsCDs._raidCDs_groupPoll_state == "pause" then
                                TehrsCDs._raidCDs_groupPoll_state = "nextPlayer"
                            else
                                TehrsCDs._raidCDs_groupPoll_state = "pause"
                            end
                            print("|cFF00A2E8Tehr's RaidCDs:|r "..buttonText["L1N8"].." "..(TehrsCDs._raidCDs_groupPoll_state == "pause" and "|cFFFF0000"..buttonText["printDisabled"].."|r" or "|cFF00FF00"..buttonText["printEnabled"].."|r" ))
                    end },                        
                    --[[
                --Toggle Sorting by Timer
                { text = buttonText["L1N9"], isNotRadio = true, keepShownOnClick = true, checked = TehrsCDs["Show Settings"].SortByTimer,
                    func = function()
                        TehrsCDs["Show Settings"].SortByTimer = not TehrsCDs["Show Settings"].SortByTimer
                        print("|cFF00A2E8Tehr's RaidCDs:|r "..buttonText["L1N9"].." "..(not TehrsCDs["Show Settings"].SortByTimer and "|cFFFF0000"..buttonText["printDisabled"].."|r" or "|cFF00FF00"..buttonText["printEnabled"].."|r" ))
                    end },                    
                        ]]
                    
                    --Toggle Empty Sections
                    { text = buttonText["L1N10"], isNotRadio = true, keepShownOnClick = true, checked = TehrsCDs["Show Settings"].ShowEmptySections,
                        func = function()
                            TehrsCDs["Show Settings"].ShowEmptySections = not TehrsCDs["Show Settings"].ShowEmptySections
                            print("|cFF00A2E8Tehr's RaidCDs:|r "..buttonText["L1N10"].." "..( TehrsCDs["Show Settings"].ShowEmptySections and "|cFF00FF00"..buttonText["printEnabled"].."|r" or "|cFFFF0000"..buttonText["printDisabled"].."|r" ))
                    end },          
                    
                    --Toggle On Cool
                    { text = buttonText["L1N11"], isNotRadio = true, keepShownOnClick = true, checked = TehrsCDs["Show Settings"].ShowOnCooldownOnly,
                        func = function()
                            TehrsCDs["Show Settings"].ShowOnCooldownOnly = not TehrsCDs["Show Settings"].ShowOnCooldownOnly
                            print("|cFF00A2E8Tehr's RaidCDs:|r "..buttonText["L1N11"].." "..( TehrsCDs["Show Settings"].ShowOnCooldownOnly and "|cFF00FF00"..buttonText["printEnabled"].."|r" or "|cFFFF0000"..buttonText["printDisabled"].."|r" ))
                    end },                    
                    
                    { text = " ", notCheckable = 1, disabled = true},
                    
                    --General Settings
                    { text = buttonText["L1N4"], keepShownOnClick = true, hasArrow = true, notCheckable = 1,
                        menuList = {                
                            { text = buttonText["L2N1.1"], notCheckable = 1, isTitle = true}, 
                            { text = buttonText["L2N2.1"], notCheckable = 1,
                                func = function()
                                    TehrsCDs["Show Settings"].allExterns = true
                                    TehrsCDs["Show Settings"].allCDs = true
                                    TehrsCDs["Show Settings"].allUtility = true
                                    TehrsCDs["Show Settings"].allImmunityCDs = true
                                    TehrsCDs["Show Settings"].allAoECCs = true
                                    TehrsCDs["Show Settings"].allInterrupts = true
                                    TehrsCDs["Show Settings"].allRezzes = true
                                    print("|cFF00A2E8Tehr's RaidCDs:|r All CDs |cFF00FF00"..buttonText["printEnabled"].."|r") 
                            end },
                            { text = " ", notCheckable = 1, disabled = true},
                            { text = buttonText["L2N3.1"], isNotRadio = true, keepShownOnClick = true, checked = TehrsCDs["Show Settings"].allExterns,
                                func = function()
                                    TehrsCDs["Show Settings"].allExterns = not TehrsCDs["Show Settings"].allExterns
                                    print("|cFF00A2E8Tehr's RaidCDs:|r "..buttonText["L2N3.4"].." "..( TehrsCDs["Show Settings"].allExterns and "|cFF00FF00"..buttonText["printEnabled"].."|r" or "|cFFFF0000"..buttonText["printDisabled"].."|r" )) 
                            end },
                            { text = buttonText["L2N4.1"], isNotRadio = true, keepShownOnClick = true, checked = TehrsCDs["Show Settings"].allCDs,
