local TehrsCDs_show_ParentButton = true --Do you want the button? 

--------------------------------------------
----- Don't change anything below this -----
--------------------------------------------

local parentID = WeakAurasSaved.displays[WeakAuras.regions[aura_env.id].region:GetParent().id]
if (parentID["TehrsRaidCDs"] == nil) then parentID["TehrsRaidCDs"] = {} end    
if (parentID["TehrsRaidCDs"]["Show Settings"] == nil) then parentID["TehrsRaidCDs"]["Show Settings"] = {} end
if (parentID["TehrsRaidCDs"]["Translation Settings"] == nil) then parentID["TehrsRaidCDs"]["Translation Settings"] = {} end
if (parentID["TehrsRaidCDs"]["Translation Settings"]["frFR"] == nil) then parentID["TehrsRaidCDs"]["Translation Settings"]["frFR"] = {} end
if (parentID["TehrsRaidCDs"]["Translation Settings"]["zhCN"] == nil) then parentID["TehrsRaidCDs"]["Translation Settings"]["zhCN"] = {} end
if (parentID["TehrsRaidCDs"]["Translation Settings"]["zhTW"] == nil) then parentID["TehrsRaidCDs"]["Translation Settings"]["zhTW"] = {} end
if (parentID["TehrsRaidCDs"]["Translation Settings"]["deDE"] == nil) then parentID["TehrsRaidCDs"]["Translation Settings"]["deDE"] = {} end
if (parentID["TehrsRaidCDs"]["Translation Settings"]["ruRU"] == nil) then parentID["TehrsRaidCDs"]["Translation Settings"]["ruRU"] = {} end
if (parentID["TehrsRaidCDs"]["Translation Settings"]["koKR"] == nil) then parentID["TehrsRaidCDs"]["Translation Settings"]["koKR"] = {} end
if (parentID["TehrsRaidCDs"]["Translation Settings"]["esES"] == nil) then parentID["TehrsRaidCDs"]["Translation Settings"]["esES"] = {} end
if (parentID["TehrsRaidCDs"]["Translation Settings"]["itIT"] == nil) then parentID["TehrsRaidCDs"]["Translation Settings"]["itIT"] = {} end
local TehrsCDs = parentID["TehrsRaidCDs"]        
local backgroundName = parentID["controlledChildren"][6]
local namesTextName = parentID["controlledChildren"][3]
local filename, fontHeight, flags = WeakAuras.regions[namesTextName].region.text:GetFont()
local backgroundAlpha = "Background Alpha"
local textSize = "Text Size"
local locale = GetLocale()
local RetrieveSpellName = function(spellID)
    local name, rank, icon, castingTime, minRange, maxRange, spellID = GetSpellInfo(spellID) or "something broke"
    return name
end
local buttonText = { }
TehrsCDs.fontSize = TehrsCDs.fontSize or 0    

if ((not TehrsCDs["Show Settings"].allExterns) and (not TehrsCDs["Show Settings"].allCDs) and (not TehrsCDs["Show Settings"].allUtility) and (not TehrsCDs["Show Settings"].allImmunityCDs) and (not TehrsCDs["Show Settings"].allAoECCs) and (not TehrsCDs["Show Settings"].allInterrupts) and (not TehrsCDs["Show Settings"].allRezzes)) then
    print("|cFF00A2E8Tehr's RaidCDs:|r If you disabled all of the sections on accident and want to re-enable them, type the following:\n   /script WeakAuras.ScanEvents(\"TehrsCDs_ShowAll\")")
end

-- Translation Handler
if #parentID["controlledChildren"] > 8 then
    for i = 8,#parentID["controlledChildren"] do
        if parentID["controlledChildren"][i] then
            WeakAuras.ActivateAuraEnvironment(parentID["controlledChildren"][i])
            WeakAuras.ActivateAuraEnvironment()
        end
    end
end

if TehrsCDs["Translation Settings"]["frFR"].Translation_French then
    buttonText = TehrsCDs["Translation Settings"]["frFR"].Translation_French_buttonText
elseif TehrsCDs["Translation Settings"]["zhCN"].Translation_Chinese then
